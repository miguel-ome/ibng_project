generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model user {
  id         Int    @id @default(autoincrement())
  question1  String
  question2  String
  question3  String
  question4  String
  question5  String
  question6  String
  question7  String
  question8  String
  question9  String
  question10 String
  question11 String
  question12 String
  question13 String
  question14 String
}
